{
  "name": "OKC Show Likes",
  "description": "Extension that shows likes in OKC for free",
  "version": "1.1",
  "manifest_version": 3,
  "permissions": ["nativeMessaging", "storage", "tabs"],
  "host_permissions": ["https://www.okcupid.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.okcupid.com/*"],
      "js": ["script.js"],
      "css": ["style.css"]
    }
  ]
}
